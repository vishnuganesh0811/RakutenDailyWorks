
public class Customer {
	private int Id;
	private String Name;
	private String Address;
	private String Email;
	private int Phone;
	static int counter;
	
	public Customer(int id, String name, String address, String email, int phone) {
		super();
		this.Id = generateId();
		this.Name = name;
		this.Address = address;
		this.Email = email;
		this.Phone = phone;
	}
	
	
	private int generateId()
	{
		return counter++;
	}

	
	

}
