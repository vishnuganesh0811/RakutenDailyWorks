
public class Customer {
	private int Id;
	private String Name;
	private String Address;
	private String Email;
	private int Phone;
	
	public Customer(int id, String name, String address, String email, int phone) {
		super();
		Id = id;
		Name = name;
		Address = address;
		Email = email;
		Phone = phone;
	}
	
	
	

	
	

}
